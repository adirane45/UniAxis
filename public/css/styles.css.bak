/* =====================================
   PROFESSIONAL ENHANCEMENTS
   ===================================== */

/* Smooth scrollbar styling */
::-webkit-scrollbar {
    width: 12px;
}

::-webkit-scrollbar-track {
    background: var(--bg-secondary);
}

::-webkit-scrollbar-thumb {
    background: var(--border-color);
    border-radius: 6px;
}

::-webkit-scrollbar-thumb:hover {
    background: var(--text-tertiary);
}

body.dark-mode ::-webkit-scrollbar-track {
    background: var(--bg-primary);
}

body.dark-mode ::-webkit-scrollbar-thumb {
    background: var(--border-color);
}

body.dark-mode ::-webkit-scrollbar-thumb:hover {
    background: var(--text-tertiary);
}

/* Selection colors */
::selection {
    background: var(--primary-color);
    color: #ffffff;
}

::-moz-selection {
    background: var(--primary-color);
    color: #ffffff;
}

/* Focus visible for accessibility */
*:focus-visible {
    outline: 2px solid var(--primary-color);
    outline-offset: 2px;
}

/* Improved link hover states */
a {
    position: relative;
}

/* Loading skeleton for better UX */
.skeleton {
    background: linear-gradient(90deg, var(--bg-secondary) 25%, var(--bg-tertiary) 50%, var(--bg-secondary) 75%);
    background-size: 200% 100%;
    animation: skeleton-loading 1.5s ease-in-out infinite;
}

@keyframes skeleton-loading {
    0% { background-position: 200% 0; }
    100% { background-position: -200% 0; }
}

/* Better focus states for forms */
body.dark-mode input:focus,
body.dark-mode textarea:focus,
body.dark-mode select:focus,
body.dark-mode button:focus {
    outline:none;
}

/* Improve card shadows in dark mode */
body.dark-mode .service-card,
body.dark-mode .product-showcase,
body.dark-mode .team-card {
    box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.3), 0 2px 4px -1px rgba(0, 0, 0, 0.2);
}

body.dark-mode .service-card:hover,
body.dark-mode .team-card:hover {
    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.4), 0 10px 10px -5px rgba(0, 0, 0, 0.3);
}

/* Enhanced gradient overlays for dark mode */
body.dark-mode .hero {
    background: linear-gradient(135deg, #1e293b 0%, #0f172a 100%);
}

body.dark-mode .hero-overlay {
    background: linear-gradient(135deg, 
        rgba(79, 70, 229, 0.15) 0%, 
        rgba(16, 185, 129, 0.15) 100%);
}

/* Better hover effects on interactive elements */
.service-card,
.product-showcase,
.team-card,
.testimonial-card,
.comparison-item {
    will-change: transform;
}

/* Improve text readability */
body.dark-mode {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

/* Better spacing and typography */
h1, h2, h3, h4, h5, h6 {
    font-weight: 700;
    line-height: 1.2;
    letter-spacing: -0.02em;
}

p {
    line-height: 1.7;
}

/* Improve button accessibility */
button:disabled,
.btn:disabled {
    opacity: 0.6;
    cursor: not-allowed;
}

/* Enhanced card hover states */
.service-card:hover,
.product-showcase:hover,
.team-card:hover {
    transform: translateY(-6px);
    transition: transform 0.3s ease, box-shadow 0.3s ease;
}

/* Better mobile tap targets */
@media (max-width: 768px) {
    button, .btn, a[role="button"] {
        min-height: 44px;
        min-width: 44px;
    }
}

/* Smooth image loading */
img {
    image-rendering: -webkit-optimize-contrast;
    image-rendering: crisp-edges;
}

/* Professional animations */
@keyframes fadeIn {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}

@keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

@keyframes slideUp {
    from {
        transform: translateY(100%);
        opacity: 0;
    }
    to {
        transform: translateY(0);
        opacity: 1;
    }
}

@keyframes slideInRight {
    from {
        transform: translateX(100%);
        opacity: 0;
    }
    to {
        transform: translateX(0);
        opacity: 1;
    }
}

/* Performance optimizations */
.hero,
.service-card,
.product-showcase {
    contain: layout style paint;
}

/* Dark mode transitions */
body.dark-mode * {
    transition: background-color 0.3s ease, 
                color 0.3s ease, 
                border-color 0.3s ease,
                box-shadow 0.3s ease;
}

/* Disable transitions on page load */
.preload * {
    transition: none !important;
}


