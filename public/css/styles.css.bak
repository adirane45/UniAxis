/* =====================================
   SCROLL & PERFORMANCE FIXES
   ===================================== */

/* Ensure smooth scrolling doesn't conflict */
html {
    scroll-behavior: smooth;
    scroll-padding-top: 80px; /* Account for fixed navbar */
}

/* Prevent layout shift from scrollbar */
html {
    overflow-y: scroll;
}

/* Fix for navbar always staying on top */
.navbar {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1000;
    will-change: transform;
}

/* Ensure scrollToTop button doesn't interfere */
#scrollToTop {
    pointer-events: auto;
    will-change: transform, opacity;
}

#scrollToTop:not(.show) {
    pointer-events: none;
    opacity: 0;
}

/* Fix WhatsApp button position */
.whatsapp-float {
    pointer-events: auto;
    will-change: transform;
}

/* Prevent text selection during scroll */
body.scrolling {
    user-select: none;
}

/* Fix for iOS bounce scroll */
body {
    overscroll-behavior-y: none;
}

/* Optimize animations */
@media (prefers-reduced-motion: reduce) {
    *,
    *::before,
    *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
        scroll-behavior: auto !important;
    }
}

/* Fix z-index stacking context */
.navbar { z-index: 1000; }
#scrollToTop { z-index: 999; }
.whatsapp-float { z-index: 998; }
.social-share-floating { z-index: 997; }
.video-modal { z-index: 10002; }
.search-modal { z-index: 10001; }
.lightbox { z-index: 10000; }

/* Prevent modal backdrop scroll issues */
.modal-open {
    overflow: hidden !important;
}

body.modal-open {
    overflow: hidden !important;
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
}

/* GPU acceleration for smooth performance */
.service-card,
.product-showcase,
.team-card,
.testimonial-card,
#scrollToTop,
.whatsapp-float,
.navbar {
    transform: translateZ(0);
    backface-visibility: hidden;
}

/* Fix mobile momentum scrolling */
@media (max-width: 768px) {
    body {
        -webkit-overflow-scrolling: touch;
    }
}

/* Optimize hover effects */
@media (hover: hover) and (pointer: fine) {
    .service-card:hover,
    .team-card:hover,
    .product-showcase:hover {
        transform: translateY(-6px) translateZ(0);
    }
}

/* Fix for Safari sticky navbar */
@supports (-webkit-appearance: none) {
    .navbar {
        -webkit-transform: translateZ(0);
    }
}

/* Prevent flash of unstyled content */
.preload * {
    transition: none !important;
    animation: none !important;
}

/* Ensure sections don't overlap navbar */
section {
    position: relative;
    z-index: 1;
}

/* Fix for anchor links with fixed header - use scroll-margin instead */
section[id] {
    scroll-margin-top: 80px;
}

/* Remove blank space workaround */
.hero {
    scroll-margin-top: 0;
}


/* =====================================
   MOBILE MENU FIXES
   ===================================== */

/* Fix mobile menu scroll issues */
.nav-menu.active {
    position: fixed;
    top: 70px;
    left: 0;
    right: 0;
    bottom: 0;
    overflow-y: auto;
    overscroll-behavior: contain;
    -webkit-overflow-scrolling: touch;
}

/* Prevent body scroll when mobile menu is open */
body.menu-open {
    overflow: hidden !important;
    position: fixed;
    width: 100%;
}

@media (max-width: 768px) {
    /* Ensure mobile menu doesn't block scroll when closed */
    body:not(.menu-open) {
        overflow-x: hidden;
        overflow-y: auto !important;
    }
}

/* =====================================
   ADDITIONAL BUG FIXES
   ===================================== */

/* Fix for images not loading properly */
img {
    max-width: 100%;
    height: auto;
    display: block;
}

/* Fix for form inputs on iOS */
input,
textarea,
select {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}

/* Prevent zoom on input focus (iOS) */
@media (max-width: 768px) {
    input[type="text"],
    input[type="email"],
    input[type="tel"],
    textarea {
        font-size: 16px !important;
    }
}

/* Fix for button click delay on mobile */
button,
a,
label,
input[type="submit"],
input[type="button"] {
    touch-action: manipulation;
}

/* Fix for text antialiasing */
body {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    text-rendering: optimizeLegibility;
}

/* Fix for broken images */
img::before {
    content: '';
    display: block;
}

/* Fix for video aspect ratio */
iframe {
    max-width: 100%;
}

/* Prevent horizontal scroll */
body, html {
    max-width: 100%;
    overflow-x: hidden;
}

/* Fix for long words breaking layout */
p, li, td, th, div {
    word-wrap: break-word;
    overflow-wrap: break-word;
}

/* Fix for code blocks */
pre, code {
    white-space: pre-wrap;
    word-wrap: break-word;
}

/* Accessibility: Focus indicators */
a:focus-visible,
button:focus-visible,
input:focus-visible,
textarea:focus-visible {
    outline: 2px solid var(--primary-color);
    outline-offset: 2px;
}

/* Fix for sticky hover on touch devices */
@media (hover: none) {
    .service-card:hover,
    .team-card:hover,
    .product-showcase:hover {
        transform: none;
    }
}

/* Optimize font loading */
@font-face {
    font-display: swap;
}

/* Fix for print styles */
@media print {
    .navbar,
    #scrollToTop,
    .whatsapp-float,
    .social-share-floating {
        display: none !important;
    }
    
    body {
        overflow: visible !important;
    }
}

/* Fix for contrast in light mode */
body:not(.dark-mode) {
    color: var(--text-primary);
}

/* Ensure buttons are clickable */
button {
    cursor: pointer;
    user-select: none;
}

button:disabled {
    cursor: not-allowed;
    opacity: 0.6;
}

/* Fix for Safari date input */
input[type="date"]::-webkit-calendar-picker-indicator {
    cursor: pointer;
}

/* Fix for number input spinner */
input[type="number"]::-webkit-inner-spin-button,
input[type="number"]::-webkit-outer-spin-button {
    opacity: 1;
}


/* =====================================
   REDESIGNED ABOUT SECTION
   ===================================== */

/* About Section Modern Redesign */
.about {
    background: var(--bg-secondary);
    position: relative;
    padding: 80px 0;
}

.about .section-title {
    margin-bottom: 16px;
}

.about .section-subtitle {
    margin-bottom: 60px;
}

.about-content {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: start;
    margin-bottom: 60px;
}

.about-text {
    display: flex;
    flex-direction: column;
    gap: 24px;
}

.about-card {
    background: var(--bg-primary);
    padding: 32px;
    border-radius: 16px;
    border: 1px solid var(--border-color);
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
}

.about-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 4px;
    height: 100%;
    background: linear-gradient(135deg, var(--primary-color), var(--secondary-color));
    opacity: 0;
    transition: opacity 0.3s ease;
}

.about-card:hover {
    transform: translateX(8px);
    border-color: var(--primary-color);
    box-shadow: var(--shadow-lg);
}

.about-card:hover::before {
    opacity: 1;
}

.about-card .card-icon {
    font-size: 3rem;
    margin-bottom: 16px;
    display: block;
}

.about-card h3 {
    color: var(--text-primary);
    font-size: 1.5rem;
    margin-bottom: 12px;
    font-weight: 700;
}

.about-card p {
    color: var(--text-secondary);
    font-size: 1rem;
    line-height: 1.7;
    margin: 0;
}

.about-image {
    width: 100%;
    height: 600px;
    border-radius: 20px;
    overflow: hidden;
    box-shadow: var(--shadow-xl);
    position: relative;
    background: linear-gradient(135deg, var(--primary-color) 0%, var(--secondary-color) 100%);
}

.about-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.5s ease;
}

.about-image:hover img {
    transform: scale(1.05);
}

.image-overlay {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: linear-gradient(to top, rgba(0, 0, 0, 0.8) 0%, transparent 100%);
    padding: 40px 32px;
    transform: translateY(100%);
    transition: transform 0.3s ease;
}

.about-image:hover .image-overlay {
    transform: translateY(0);
}

.overlay-content h4 {
    color: #ffffff;
    font-size: 1.8rem;
    margin-bottom: 8px;
    font-weight: 700;
}

.overlay-content p {
    color: rgba(255, 255, 255, 0.9);
    font-size: 1.1rem;
    margin: 0;
}

/* About Stats Row */
.about-stats-row {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 24px;
    margin-bottom: 60px;
}

.about-stat-card {
    background: var(--bg-primary);
    padding: 32px 24px;
    border-radius: 16px;
    text-align: center;
    border: 2px solid var(--border-color);
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
}

.about-stat-card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));
    transform: scaleX(0);
    transition: transform 0.3s ease;
}

.about-stat-card:hover {
    transform: translateY(-8px);
    border-color: var(--primary-color);
    box-shadow: var(--shadow-lg);
}

.about-stat-card:hover::before {
    transform: scaleX(1);
}

.stat-icon-bg {
    font-size: 3rem;
    margin-bottom: 12px;
    display: inline-block;
}

.about-stat-card .stat-number {
    font-size: 2.8rem;
    font-weight: 800;
    color: var(--primary-color);
    margin-bottom: 8px;
    line-height: 1;
}

.about-stat-card .stat-label {
    color: var(--text-secondary);
    font-size: 0.95rem;
    font-weight: 600;
}

/* Why Choose Us Section */
.why-choose-us {
    margin-top: 80px;
}

.subsection-title {
    font-size: 2rem;
    font-weight: 700;
    color: var(--text-primary);
    text-align: center;
    margin-bottom: 48px;
    position: relative;
    display: inline-block;
    width: 100%;
}

.subsection-title::after {
    content: '';
    position: absolute;
    bottom: -12px;
    left: 50%;
    transform: translateX(-50%);
    width: 80px;
    height: 4px;
    background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));
    border-radius: 2px;
}

.values-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
}

.value-card {
    background: var(--bg-primary);
    padding: 36px 28px;
    border-radius: 16px;
    text-align: center;
    border: 2px solid var(--border-color);
    transition: all 0.3s ease;
    position: relative;
}

.value-card::before {
    content: '';
    position: absolute;
    inset: 0;
    border-radius: 16px;
    padding: 2px;
    background: linear-gradient(135deg, var(--primary-color), var(--secondary-color));
    -webkit-mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
    -webkit-mask-composite: xor;
    mask-composite: exclude;
    opacity: 0;
    transition: opacity 0.3s ease;
}

.value-card:hover {
    transform: translateY(-8px);
    box-shadow: var(--shadow-xl);
}

.value-card:hover::before {
    opacity: 1;
}

.value-card .value-icon {
    font-size: 3.5rem;
    margin-bottom: 16px;
    display: block;
}

.value-card h4 {
    color: var(--text-primary);
    font-size: 1.3rem;
    margin-bottom: 12px;
    font-weight: 700;
}

.value-card p {
    color: var(--text-secondary);
    font-size: 0.95rem;
    line-height: 1.6;
    margin: 0;
}

/* Dark Mode Adjustments for About Section */
body.dark-mode .about-card,
body.dark-mode .about-stat-card,
body.dark-mode .value-card {
    background: var(--bg-tertiary);
    border-color: var(--border-color);
}

body.dark-mode .about-card:hover,
body.dark-mode .about-stat-card:hover,
body.dark-mode .value-card:hover {
    border-color: var(--primary-light);
}

body.dark-mode .about-image {
    background: linear-gradient(135deg, #4338ca 0%, #6366f1 100%);
}

/* Dark mode for modern about section */
body.dark-mode .mv-card,
body.dark-mode .stat-card-modern,
body.dark-mode .benefit-card {
    background: var(--bg-tertiary);
    border-color: var(--border-color);
}

body.dark-mode .mv-card:hover,
body.dark-mode .stat-card-modern:hover,
body.dark-mode .benefit-card:hover {
    border-color: var(--primary-light);
}

body.dark-mode .why-choose-modern {
    background: var(--bg-tertiary);
}

body.dark-mode .image-badge {
    background: rgba(30, 30, 40, 0.95);
}

/* Responsive Design for About Section */
@media (max-width: 968px) {
    .about-content {
        grid-template-columns: 1fr;
        gap: 40px;
    }
    
    .about-image {
        height: 400px;
        order: -1;
    }
    
    .about-stats-row {
        grid-template-columns: repeat(2, 1fr);
    }
    
    .values-grid {
        grid-template-columns: repeat(2, 1fr);
        gap: 24px;
    }
}

@media (max-width: 768px) {
    .about {
        padding: 60px 0;
    }
    
    .about-overview {
        grid-template-columns: 1fr;
        gap: 30px;
    }
    
    .overview-image {
        height: 300px;
    }
    
    .mission-vision-grid {
        grid-template-columns: 1fr;
    }
    
    .about-stats-modern {
        grid-template-columns: 1fr;
        gap: 16px;
    }
    
    .benefits-grid {
        grid-template-columns: 1fr;
    }
    
    .why-choose-modern {
        padding: 30px 20px;
    }
    
    .about-stats-row {
        grid-template-columns: 1fr;
        gap: 16px;
    }
    
    .values-grid {
        grid-template-columns: 1fr;
    }
    
    .about-card {
        padding: 24px;
    }
    
    .value-card {
        padding: 28px 20px;
    }
}


/* =====================================
   FINAL SCROLL FIX SAFEGUARDS
   ===================================== */

/* Ensure scroll works */
html {
    min-height: 100vh;
}

/* Only block scroll when modal or menu is open */
body.modal-open,
body.menu-open {
    overflow: hidden !important;
    position: fixed !important;
    width: 100%;
}
